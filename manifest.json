{
  "manifest_version": 2,

  "name": "Moodle Downloader",
  "description": "A chrome extension for downloading Moodle resources",
  "version": "0.2",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "downloads"
  ]
}
